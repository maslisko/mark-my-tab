{
  "manifest_version": 2,
  "name": "Mark My Tab",
  "version": "1.0",
  "description": "Marks tabs with one of the prefefined icons",
  "homepage_url": "https://github.com/maslisko/mark-my-tab",
  "icons": {
    "48": "icons/markMyTab-48.png",
    "256": "icons/markMyTab-256.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "markMyTab.js"
      ]
    }
  ]
}